<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.softweb.npk.fragment.HomeFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:weightSum="6"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_recomendacoes_adubacao"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent" />

                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />


            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin">

                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:onClick="microNutrientes"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_recomedacao_micronutrientes"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:layout_height="match_parent" />

                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco"
                    android:onClick="microNutrientes"
                    android:id="@+id/button" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">

                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_gesso"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:onClick="recGesso" />

                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco"
                    android:onClick="recGesso" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_fertirrigacao"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal" />
                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_fertCafConilon"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal" />
                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_cultOrganico"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal" />
                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_analiseSolo"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal" />
                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:weightSum="10"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:background="@color/branco">
                <Button
                    style="@android:style/Widget.Button"
                    android:textColor="@color/txtButtom"
                    android:background="@color/branco"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_width="match_parent"
                    android:text="@string/btn_analiseFoliar"
                    android:textSize="@dimen/tamanho_subtitulo"
                    android:gravity="center_vertical|center_horizontal"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal" />
                <Button
                    android:layout_weight="9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=">"
                    android:background="@color/colorPrimaryDark"
                    android:textSize="@dimen/tamanho_titulo"
                    android:textColor="@color/branco" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</FrameLayout>
